<table class="table table-hover">
  <thead class="thead-light">
    <tr>
      <th>Title</th>
      <th colspan="1"></th>
    </tr>
  </thead>
  <tbody>
  <% @parts.each do |part| %>
    <% if part.id.present? %>
      <tr>
        <td><%= link_to part.title, project_subject_part_path(@project, @subject, part), class: "part-title" %></td>
        <td class="text-right"><%= link_to 'Edit', edit_project_subject_part_path(@project, @subject, part), class: "btn btn-outline-info btn-sm" %>
        <%= link_to 'Destroy', project_subject_part_path(@project, @subject, part), method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-outline-info btn-sm" %></td>
      </tr>
    <% end %>
    <% end %>
  </tbody>
</table>
