<h1>Parts</h1>

<table class="table table-hover">
  <thead class="thead-light">
    <tr>
      <th>Title</th>
      <th>Content</th>
    </tr>
  </thead>
  <tbody>
  <% parts.each do |part| %>
    <% if part.id.present? %>
      <tr>
        <td><%= link_to part.title, project_subject_part_path(@project, @subject, part) %></td>
        <td><%= part.content %></td>
      </tr>
    <% end %>
    <% end %>
  </tbody>
</table>
