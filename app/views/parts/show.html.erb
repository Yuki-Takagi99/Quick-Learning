<% if user_signed_in? %>
  <%= stylesheet_link_tag "user", :media => "all" %>
<% end %>
<div class="main-wrap">
  <div class="main">
    <h3>Part Show</h3>
    <div class="main-table">
      <table class="table">
        <tbody>
          <tr>
            <th><%= @part.title %></th>
          </tr>
          <tr>
            <td><%= @part.content.html_safe %></td>
          </tr>
        </tbody>
      </table>
    </div>
    <% if admin_signed_in? %>
      <%= link_to 'Edit', edit_project_subject_part_path(@project, @subject, @part), class: 'btn btn-info btn-block btn-lg' %>
    <% end %>
  </div>
</div>
<%= link_to "Question Index", project_subject_part_questions_path(@project, @subject, @part), class: "nav-link" %>
<%= link_to 'New Question', new_project_subject_part_question_path(@project, @subject, @part, @question) %>
