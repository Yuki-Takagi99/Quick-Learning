<%= form_with(model: [project, test], local: true) do |form| %>
  <div class="form-group row">
    <%= form.text_field :title, placeholder: "input title", class: "form-control col-11" %><%= form.select :subject_id, Subject.where(project_id: admin_project.id).map { |subject| [subject.title, subject.id] }, {}, class: "form-control mt-1 col-9" %><%= form.submit nil, class: "btn btn-info btn-sm col-2 mt-1 ml-1"  %>
  </div>
<% end %>
